{
  "name": "function-overloader",
  "version": "1.4.0",
  "description": "improve overloading functions and methods in js",
  "main": "dist/index.js",
  "scripts": {
    "prettier-all": "prettier --config .prettierrc --write 'src/**/*.js'",
    "prettier-changed": "git diff --name-only HEAD | grep \".*\\.js$\" | xargs prettier --config .prettierrc --write",
    "test": "mocha --require source-map-support --require babel-register --require babel-polyfill --require init.mocha.js --check-leaks --timeout 3000 'src/**/*.spec.js'",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --require source-map-support --require babel-register --require babel-polyfill --require init.mocha.js --check-leaks --timeout 3000 'src/**/*.spec.js' && open coverage/lcov-report/index.html",
    "complexity": "plato -r --eslint .eslintrc -d report src && open report/index.html",
    "linter": "eslint src",
    "precommit": "lint-staged && npm run linter && npm test",
    "build": "webpack --config webpack.babel.js",
    "example": "npm run build-server && node --require babel-register example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhlryk/function-overloader.git"
  },
  "keywords": [],
  "author": "Krzysztof Sztompka <sztompka.krzysztof@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uhlryk/function-overloader/issues"
  },
  "homepage": "https://github.com/uhlryk/function-overloader#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.7.2",
    "eslint-config-prettier": "^2.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^4.1.3",
    "mocha": "^3.4.2",
    "mongo-mock": "^2.4.0",
    "plato": "^1.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.6.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.13.0",
    "supertest": "^3.0.0",
    "webpack": "^3.0.0",
    "webpack2-externals-plugin": "^1.0.0"
  },
  "dependencies": {
    "debug": "^3.0.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
